<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.snake.AdministracionController">
   <children>
      <TableView fx:id="tablaUsuarios" layoutX="70.0" layoutY="119.0" onMouseClicked="#onMouseClickTabla" prefHeight="496.0" prefWidth="660.0">
        <columns>
          <TableColumn fx:id="nombreColum" prefWidth="166.0" text="Nombre" />
          <TableColumn fx:id="dniColum" prefWidth="186.0" text="DNI" />
            <TableColumn fx:id="nivelColum" prefWidth="83.0" text="Nivel" />
            <TableColumn fx:id="victoriasColum" text="Victorias" />
            <TableColumn fx:id="derrotasColum" prefWidth="78.0" text="Derrotas" />
            <TableColumn fx:id="tipoColum" prefWidth="66.0" text="Tipo" />
        </columns>
      </TableView>
      <Button fx:id="ejecutarButtonFX" layoutX="283.0" layoutY="714.0" mnemonicParsing="false" onAction="#ejecutarButton" prefHeight="36.0" prefWidth="258.0" text="Ejecutar" />
      <Button fx:id="volverButtonFX" layoutX="70.0" layoutY="650.0" mnemonicParsing="false" onAction="#salirAplicacion" prefHeight="100.0" prefWidth="100.0" text="Salir" />
      <Button fx:id="jugarButtonFX" layoutX="630.0" layoutY="650.0" mnemonicParsing="false" onAction="#jugarButton" prefHeight="100.0" prefWidth="100.0" text="Jugar" />
      <TextField fx:id="nombreField" layoutX="70.0" layoutY="69.0" prefHeight="25.0" prefWidth="166.0" promptText="Nuevo nombre" />
      <TextField fx:id="dniField" layoutX="238.0" layoutY="69.0" prefHeight="25.0" prefWidth="183.0" promptText="Nuevo DNI" />
      <TextField fx:id="nivelField" layoutX="423.0" layoutY="69.0" prefHeight="25.0" prefWidth="82.0" promptText="Nivel" />
      <TextField fx:id="victoriasField" layoutX="507.0" layoutY="69.0" prefHeight="25.0" prefWidth="77.0" promptText="Victorias" />
      <TextField fx:id="derrotasField" layoutX="586.0" layoutY="69.0" prefHeight="25.0" prefWidth="75.0" promptText="Derrotas" />
      <TextField fx:id="tipoField" layoutX="663.0" layoutY="69.0" prefHeight="25.0" prefWidth="67.0" promptText="Tipo" />
      <ComboBox fx:id="comboSeleccion" layoutX="283.0" layoutY="655.0" onAction="#actionComboBox" prefWidth="150.0" />
      <CheckBox fx:id="checkConfirmacion" layoutX="471.0" layoutY="659.0" mnemonicParsing="false" text="Confirmo" />
   </children>
</AnchorPane>
